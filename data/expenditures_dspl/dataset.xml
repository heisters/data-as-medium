  <info>
    <name>
      <value xml:lang="en">FEC Operating Expenditures</value>
    </name>
    <description>
      <value xml:lang="en">Campaign operating expenditures from 2015 - 2016 from the FEC</value>
    </description>
    <url>
      <value xml:lang="en">http://www.fec.gov/finance/disclosure/ftpdet.shtml#a2015_2016</value>
    </url>
  </info>
  <provider>
    <name>
      <value xml:lang="en">FEC</value>
    </name>
    <url>
      <value xml:lang="en">http://www.fec.gov</value>
    </url>
  </provider>
  <concepts>
    <concept id="CMTE_ID">
      <info>
        <name>
          <value xml:lang="en">Filer Identification Number</value>
        </name>
        <description>
          <value xml:lang="en">Identification number of committee filing report.</value>
        </description>
      </info>
      <type ref="string"/>
      <table ref="CMTE_ID_table"/>
    </concept>
    <concept id="AMNDT_IND">
      <info>
        <name>
          <value xml:lang="en">Amendment Indicator</value>
        </name>
        <description>
          <value xml:lang="en">Indicates if the report being filed is new (N), an amendment (A) to a previous report, or a termination (T) report.</value>
        </description>
      </info>
      <type ref="string"/>
      <table ref="AMNDT_IND_table"/>
    </concept>
    <concept id="RPT_TP">
      <info>
        <name>
          <value xml:lang="en">Report Type</value>
        </name>
        <description>
          <value xml:lang="en">Indicates the type of report filed.</value>
        </description>
      </info>
      <type ref="string"/>
      <table ref="RPT_TP_table"/>
    </concept>
    <concept id="IMAGE_NUM">
      <info>
        <name>
          <value xml:lang="en">Image Number</value>
        </name>
        <description>
          <value xml:lang="en">11-digit Image Number Format
            YYOORRRFFFF
            YY - scanning year 
            OO - office (01 - House, 02 - Senate, 03 - FEC Paper, 9x - FEC Electronic) 
            RRR - reel number 
            FFFF- frame number
            18-digit Image Number Format (June 29, 2015)
            YYYYMMDDSSPPPPPPPP
            YYYY - scanning year 
            MM - scanning month 
            DD - scanning day 
            SS - source (02 - Senate, 03 - FEC Paper, 9x - FEC Electronic) 
          PPPPPPPP - page (reset to zero every year on January 1)</value>
        </description>
      </info>
      <type ref="string"/>
      <table ref="IMAGE_NUM_table"/>
    </concept>
    <concept id="LINE_NUM">
      <info>
        <name>
          <value xml:lang="en">Line Number</value>
        </name>
        <description>
          <value xml:lang="en">Indicates FEC Form line number</value>
        </description>
      </info>
      <type ref="string"/>
      <table ref="LINE_NUM_table"/>
    </concept>
    <concept id="FORM_TP_CD">
      <info>
        <name>
          <value xml:lang="en">Form Type</value>
