        <name>
          <value xml:lang="en">Disbursement Category Code</value>
        </name>
        <description>
          <value xml:lang="en">001-012 and 101-107</value>
        </description>
      </info>
      <type ref="string"/>
      <table ref="CATEGORY_table"/>
    </concept>
    <concept id="CATEGORY_DESC">
      <info>
        <name>
          <value xml:lang="en">Disbursement Category Code Description</value>
        </name>
        <description>
